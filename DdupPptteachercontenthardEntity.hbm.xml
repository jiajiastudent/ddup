<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bean.DdupPptteachercontenthardEntity" table="ddup_pptteachercontenthard" schema="ddupmis">

            <id name="ddupTno" column="ddup_tno"/>
            <property name="ddupPptNo" column="ddup_pptNo"/>
            <property name="ddupPptPageNum" column="ddup_pptPageNum"/>

        <many-to-one name="ddupTeacherByDdupTno" class="com.bean.DdupTeacherEntity">
            <column name="ddup_tno"/>
        </many-to-one>
        <many-to-one name="ddupPptmanageByDdupPptNo" class="com.bean.DdupPptmanageEntity">
            <column name="ddup_pptNo"/>
        </many-to-one>
    </class>
</hibernate-mapping>